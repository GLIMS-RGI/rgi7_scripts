   "source": [
    "# go to every glacier differences\n",
    "def area_check(new, ref):\n",
    "    area_new = new['area'].sum()/1000000\n",
    "    area_ref = ref['area'].sum()/1000000\n",
    "    d = (area_new - area_ref)\n",
    "    d_perc = (d/area_new*100)\n",
    "    return d_perc, d"
   ]
